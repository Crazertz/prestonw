package zork;

import java.awt.Point;

public class poiint
{
	public int y;
	public int y2;
	public int x;
	public int x2;
	public poiint (int x,int y)
	{
		this.x = y;
		this.y = y;
	}
	public boolean equals(Point other) 
	{
		if(this.x == other.x && this.y == other.y)
		return true;
		else return false;
	}
	public void poiint1 (int x2,int y2)
	{
		this.x2 = y2;
		this.y2 = y2;
	}
	public boolean equals1(Point other) 
	{
		if(this.x2 == other.x && this.y2 == other.y)
		return true;
		else return false;
	}
}